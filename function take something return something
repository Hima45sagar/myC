#include<stdio.h>

int add(int, int);

int substract(int, int);

int multiple(int, int);

int main () {
    int num1, num2, s;

    printf("enter two numbers");
    scanf("%d %d", &num1, &num2);

    s = add(num1, num2);

    return 0;
}

int add(int a, int b) {
    int sum;

    sum = a + b;

    printf("sum is %d and %d : %d", sum);
    
    return(sum);

}

int substract(int a, int b) {
    int substract;

    substract = a - b;

    printf("substract is %d and %d : %d", substract);

    return(substract);
}

int multiple(int a, int b) {
    int multiple;

    multiple = a * b;

    printf("multiple is %d and %d : %d", multiple);

    return(multiple);
}
